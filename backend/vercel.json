{
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "build": {
    "env": {
      "NODE_ENV": "development"
    },
    "command": "npm run start"
  }
}